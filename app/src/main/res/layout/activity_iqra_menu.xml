<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IqraMenuActivity">

  <TextView
      android:id="@+id/textView2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="44dp"
      android:text="Iqra"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <Button
      android:id="@+id/btn_iqra_1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="240dp"
      android:text="1"
      app:layout_constraintBottom_toTopOf="@+id/btn_iqra_3"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/btn_iqra_2"
      app:layout_constraintTop_toTopOf="parent" />

  <Button
      android:id="@+id/btn_iqra_2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="240dp"
      android:text="2"
      app:layout_constraintBottom_toTopOf="@+id/btn_iqra_4"
      app:layout_constraintEnd_toStartOf="@+id/btn_iqra_1"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <Button
      android:id="@+id/btn_iqra_3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="50dp"
      android:text="3"
      app:layout_constraintBottom_toTopOf="@+id/btn_iqra_5"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/btn_iqra_4"
      app:layout_constraintTop_toBottomOf="@+id/btn_iqra_1" />

  <Button
      android:id="@+id/btn_iqra_4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="50dp"
      android:text="4"
      app:layout_constraintBottom_toTopOf="@+id/btn_iqra_6"
      app:layout_constraintEnd_toStartOf="@+id/btn_iqra_3"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btn_iqra_2" />

  <Button
      android:id="@+id/btn_iqra_5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="50dp"
      android:layout_marginBottom="307dp"
      android:text="5"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/btn_iqra_6"
      app:layout_constraintTop_toBottomOf="@+id/btn_iqra_3" />

  <Button
      android:id="@+id/btn_iqra_6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="50dp"
      android:layout_marginBottom="307dp"
      android:text="6"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/btn_iqra_5"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btn_iqra_4" />
</androidx.constraintlayout.widget.ConstraintLayout>