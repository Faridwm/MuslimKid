<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IqraMenuActivity">

  <ImageView
      android:id="@+id/imageView2"
      android:layout_width="match_parent"
      android:layout_height="200dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/upper_round" />

  <ImageButton
      android:id="@+id/btn_iqra_1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="100dp"
      android:background="@drawable/border"
      android:elevation="10dp"
      android:src="@drawable/arab_1"
      app:layout_constraintBottom_toTopOf="@+id/tv_iqra_1"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/btn_iqra_2"
      app:layout_constraintTop_toTopOf="parent" />

  <ImageButton
      android:id="@+id/btn_iqra_2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="100dp"
      android:background="@drawable/border"
      android:elevation="10dp"
      android:src="@drawable/arab_2"
      app:layout_constraintBottom_toTopOf="@+id/tv_iqra_2"
      app:layout_constraintEnd_toStartOf="@+id/btn_iqra_1"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <ImageButton
      android:id="@+id/btn_iqra_3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="32dp"
      android:background="@drawable/border"
      android:elevation="10dp"
      android:src="@drawable/arab_3"
      app:layout_constraintBottom_toTopOf="@+id/tv_iqra_3"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/btn_iqra_4"
      app:layout_constraintTop_toBottomOf="@+id/tv_iqra_1" />

  <ImageButton
      android:id="@+id/btn_iqra_4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="32dp"
      android:background="@drawable/border"
      android:elevation="10dp"
      android:src="@drawable/arab_4"
      app:layout_constraintBottom_toTopOf="@+id/tv_iqra_4"
      app:layout_constraintEnd_toStartOf="@+id/btn_iqra_3"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tv_iqra_2" />

  <ImageButton
      android:id="@+id/btn_iqra_5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="32dp"
      android:background="@drawable/border"
      android:elevation="10dp"
      android:src="@drawable/arab_5"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/btn_iqra_6"
      app:layout_constraintTop_toBottomOf="@+id/tv_iqra_3" />

  <ImageButton
      android:id="@+id/btn_iqra_6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="32dp"
      android:background="@drawable/border"
      android:elevation="10dp"
      android:src="@drawable/arab_6"
      app:layout_constraintEnd_toStartOf="@+id/btn_iqra_5"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tv_iqra_4" />

  <TextView
      android:id="@+id/tv_iqra_1"
      android:layout_width="100dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="36dp"
      android:layout_marginTop="12dp"
      android:layout_marginEnd="60dp"
      android:fontFamily="sans-serif-medium"
      android:gravity="center"
      android:text="@string/iqra_1"
      android:textSize="18sp"
      app:layout_constraintBottom_toTopOf="@+id/btn_iqra_3"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/tv_iqra_2"
      app:layout_constraintTop_toBottomOf="@+id/btn_iqra_1" />

  <TextView
      android:id="@+id/tv_iqra_2"
      android:layout_width="100dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="60dp"
      android:layout_marginTop="12dp"
      android:layout_marginEnd="36dp"
      android:fontFamily="sans-serif-medium"
      android:gravity="center"
      android:text="@string/iqra_2"
      android:textSize="18sp"
      app:layout_constraintBottom_toTopOf="@+id/btn_iqra_4"
      app:layout_constraintEnd_toStartOf="@+id/tv_iqra_1"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btn_iqra_2" />

  <TextView
      android:id="@+id/tv_iqra_3"
      android:layout_width="100dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="36dp"
      android:layout_marginTop="12dp"
      android:layout_marginEnd="60dp"
      android:fontFamily="sans-serif-medium"
      android:gravity="center"
      android:text="@string/iqra_3"
      android:textSize="18sp"
      app:layout_constraintBottom_toTopOf="@+id/btn_iqra_5"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/tv_iqra_4"
      app:layout_constraintTop_toBottomOf="@+id/btn_iqra_3" />

  <TextView
      android:id="@+id/tv_iqra_4"
      android:layout_width="100dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="60dp"
      android:layout_marginTop="12dp"
      android:layout_marginEnd="36dp"
      android:fontFamily="sans-serif-medium"
      android:gravity="center"
      android:text="@string/iqra_4"
      android:textSize="18sp"
      app:layout_constraintBottom_toTopOf="@+id/btn_iqra_6"
      app:layout_constraintEnd_toStartOf="@+id/tv_iqra_3"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btn_iqra_4" />

  <TextView
      android:id="@+id/tv_iqra_5"
      android:layout_width="100dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="32dp"
      android:layout_marginTop="12dp"
      android:layout_marginEnd="60dp"
      android:fontFamily="sans-serif-medium"
      android:gravity="center"
      android:text="@string/iqra_5"
      android:textSize="18sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/tv_iqra_6"
      app:layout_constraintTop_toBottomOf="@+id/btn_iqra_5" />

  <TextView
      android:id="@+id/tv_iqra_6"
      android:layout_width="100dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="60dp"
      android:layout_marginTop="12dp"
      android:layout_marginEnd="32dp"
      android:fontFamily="sans-serif-medium"
      android:gravity="center"
      android:text="@string/iqra_6"
      android:textSize="18sp"
      app:layout_constraintEnd_toStartOf="@+id/tv_iqra_5"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btn_iqra_6" />

  <TextView
      android:id="@+id/tv_iqra_menu"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="sans-serif-medium"
      android:text="@string/iqra"
      android:textColor="@color/white"
      android:textSize="50sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>